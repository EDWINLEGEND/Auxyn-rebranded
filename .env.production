# Production environment settings
NODE_ENV=production

# Prevents Next.js from generating source maps in production
GENERATE_SOURCEMAP=false

# Image optimization configuration
NEXT_DISABLE_IMAGE_OPTIMIZATION=true

# Prevents Next.js from sending telemetry
NEXT_TELEMETRY_DISABLED=1

# Increases static generation timeout for complex pages
NEXT_STATIC_GENERATION_TIMEOUT=180
